{
  "extends": "next/core-web-vitals",
  "rules": {
      "semi": "warn",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "no-undef": "warn",
      "no-unreachable": "warn",
      "no-constant-condition": "warn",
      "no-empty": "warn",
      "no-irregular-whitespace": "warn",
      "no-unexpected-multiline": "warn",
      "no-unsafe-negation": "warn"
  }
}
